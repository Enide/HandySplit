<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension point="org.eclipse.ui.commands">
		<command id="com.akikhtenko.split.split" name="Split editor" defaultHandler="com.akikhtenko.split.SplitProcessor"/>
   </extension>
   <extension point="org.eclipse.ui.commands">
		<command id="com.akikhtenko.split.move" name="Move editor" defaultHandler="com.akikhtenko.split.MoveProcessor"/>
   </extension>

	<extension point="org.eclipse.ui.bindings">
		<key
			commandId="com.akikhtenko.split.split"
			contextId="org.eclipse.ui.contexts.window"
			categoryId="org.eclipse.ui.category.navigate"
			sequence="M1+4 S"
			schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
	</extension>
	<extension point="org.eclipse.ui.bindings">
		<key
			commandId="com.akikhtenko.split.move"
			contextId="org.eclipse.ui.contexts.window"
			categoryId="org.eclipse.ui.category.navigate"
			sequence="M1+4 M"
			schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
	</extension>

</plugin>
